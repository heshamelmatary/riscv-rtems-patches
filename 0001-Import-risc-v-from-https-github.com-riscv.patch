+// system.
+
+void* sbrk(ptrdiff_t incr)
+{
+  static unsigned long heap_end;
+
+  if (heap_end == 0) {
+    long brk = syscall_errno(SYS_brk, 0, 0, 0, 0);
+    if(brk == -1)
+	    return (void*)-1;
+    heap_end = brk;
+  }
